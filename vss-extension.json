{
    "manifestVersion": 1,
    "id": "project-forecast-extensions",
    "version": "0.0.2",
    "name": "Project Forecasting Extensions",
    "description": "Predict the future better with evidence based project forecasting. Use your team's historical progress to get a quick and accurate prediction of how much more work there is in your backlog.",
    "publisher": "headforwards",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "prediction",
        "forecasting",
        "progress",
        "dashboard"
    ],
    "icons": {
        "default": "static/img/headforwards-logo.png"
    },
    "scopes": [
        "vso.work"
    ],
    "content": {
        "details": {
            "path": "static/docs/description.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://www.headforwards.com"
        },
        "issues": {
            "uri": "https://github.com/headforwards/ForecastVSTSExtension/issues"
        },
        "repository": {
            "uri": "https://github.com/headforwards/ForecastVSTSExtension"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/headforwards/ForecastVSTSExtension"
    },
    "badges": [
        {
            "href": "https://travis-ci.org/headforwards/ForecastVSTSExtensions",
            "uri": "https://travis-ci.org/headforwards/ForecastVSTSExtensions.svg?branch=master",
            "description": "TravisCI build for the project"
        }
    ],
    "contributions": [{
            "id": "AverageVelocityWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Average Velocity Widget",
                "description": "Average Velocity Dashboard Widget",
                "catalogIconUrl:": "static/img/blank-preview.png",
                "previewImageUrl": "static/img/blank-preview.png",
                "uri": "static/average-velocity-widget.html",
                "isNameConfigurable": false,
                "supportedSizes": [{
                        "rowSpan": 1,
                        "columnSpan": 1
                    }
                ],
                "supportedScopes": ["project_team"]
            }
        }
    ],
    "files": [{
            "path": "static/average-velocity-widget.html",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "src",
            "addressable": true
        },
        {
            "path": "static/docs",
            "addressable": true
        },
        {
            "path": "static/img",
            "addressable": true
        },
        {
            "path": "static/sdk",
            "addressable": true
        }
    ]
}